{
  "name": "@foxglovetech/turbo-form",
  "version": "0.9.1",
  "description": "Enhances forms with turbolinks like declarations.",
  "main": "index.js",
  "files": [
    "dist/*.js",
    "dist/*.js.map",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/types/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvincrespo/turbo-form.git"
  },
  "keywords": [
    "hotwire",
    "turbo",
    "forms"
  ],
  "author": "alvincrespo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alvincrespo/turbo-form/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/qunit": "^2.9.0",
    "@types/webpack-env": "^1.14.0",
    "@types/multer": "^1.4.5",
    "intern": "^4.9.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-qunit": "^4.1.2",
    "karma-sauce-launcher": "^4.3.6",
    "karma-webpack": "^5.0.0",
    "multer": "^1.4.2",
    "qunit": "^2.9.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.53",
    "rollup-plugin-terser": "^7.0.2",
    "ts-loader": "^6.0.4",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "webpack": "^4.39.1"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "clean:win": "rmdir /s /q dist",
    "types": "tsc --noEmit false --declaration true --emitDeclarationOnly true --outDir dist/types",
    "build": "yarn types && rollup -c",
    "build:win": "tsc --noEmit false --outDir dist & rollup -c",
    "build:test": "tsc -b tsconfig.test.json",
    "watch": "rollup -wc",
    "start": "node src/tests/runner.js serveOnly",
    "test": "yarn build:test && karma start karma.conf.cjs",
    "test:win": "SET NODE_OPTIONS=--inspect & node src/tests/runner.js",
    "prerelease": "yarn build && git --no-pager diff && echo && npm pack --dry-run && echo && read -n 1 -p \"Look OK? Press any key to publish and commit v$npm_package_version\" && echo",
    "release": "npm publish && git commit -am \"$npm_package_name v$npm_package_version\" && git push"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.0.0"
  }
}
